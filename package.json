    "autoprefixer": "10.4.14",
    "bcrypt": "^5.1.0",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "lucide-react": "^0.263.0",
    "next": "13.4.10",
    "next-auth": "^4.22.3",
    "postcss": "8.4.26",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rippleui": "^1.12.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "devDependencies": {
