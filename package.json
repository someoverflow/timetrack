{
  "name": "timetrack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:prod": "docker build -t timetrack:prod .",
    "docker:dev": "docker build -t timetrack:dev .",
